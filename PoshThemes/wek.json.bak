{
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{ .Shell }} in {{ .Folder }}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "#26C6DA",
                    "background": "",
                    "properties": {
                        "prefix": "",
                        "postfix": " ",
                        "macos": "mac"
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#E06C75",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "postfix": " \u003c#ffffff\u003ein\u003c/\u003e ",
                        "prefix": "",
                        "root_icon": "root"
                    }
                },
                {
                    "type": "path",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#55B9C4",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "",
                        "style": "agnoster_short",
                        "home_icon": ""
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#C678DD",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_status": true,
                        "display_stash_count": true,
                        "status_colors_enabled": true,
                        "prefix": "\u003c#ffffff\u003eon\u003c/\u003e "
                    }
                },
                {
                    "type": "node",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#98C379",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_version": true,
                        "prefix": "\u003c#ffffff\u003evia\u003c/\u003e ε₧ü "
                    }
                },
                {
                    "type": "python",
                    "style": "plain",
                    "powerline_symbol": "",
                    "foreground": "#98C379",
                    "background": "",
                    "properties": {
                        "prefix": " \uE235 ",
                        "display_virtual_env": true
                    }
                },
                {
                    "type": "az",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#000000",
                    "background": "#9ec3f0",
                    "properties": {
                        "display_id": true,
                        "display_name": true,
                        "info_separator": " @ ",
                        "prefix": " \uFD03 "
                    }
                },
                {
                    "type": "dotnet",
                    "style": "plain",
                    "powerline_symbol": "",
                    "foreground": "#98C379",
                    "background": "",
                    "properties": {
                        "prefix": " \uE77F "
                    }
                },
                {
                    "type": "battery",
                    "style": "plain",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#193549",
                    "background": "",
                    "properties": {
                        "battery_icon": "",
                        "discharging_icon": "\uE231 ",
                        "charging_icon": "\uE234 ",
                        "charged_icon": "\uE22F ",
                        "color_background": true,
                        "charged_color": "",
                        "charging_color": "#40c4ff",
                        "discharging_color": "#ff5722",
                        "postfix": "\uF295 ",
                        "display_charging": true
                    }
                },
                {
                    "type": "exit",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#C94A16",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_exit_code": true,
                        "prefix": "x "
                    }
                }
            ]
        },
        {
            "type": "newline",
            "alignment": "",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": null
        },
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#63F08C",
                    "background": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "",
                        "text": "➞"
                    }
                }
            ]
        }
    ]
}