{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [{
            "type": "prompt",
            "alignment": "left",
            "segments": [{
                    "type": "path",
                    "style": "diamond",
                    "foreground": "#ffffff",
                    "background": "#33658A",
                    "leading_diamond": "╭─",
                    "trailing_diamond": "\uE0B0",
                    "properties": {
                        "prefix": "  ",
                        "home_icon": "~",
                        "folder_separator_icon": "/",
                        "folder_icon": "..",
                        "style": "mixed",
                        "mixed_threshold": 8
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#193549",
                    "background": "#5C6E8A",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFFB38{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#C5E478{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#C792EA{{ end }}"
                    ],
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "fetch_status": true,
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true,
                        "branch_max_length": 25,
                        "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
                    }
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_version": true,
                        "prefix": "  "
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "properties": {
                        "prefix": " \uF898 ",
                        "display_version": true
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#111111",
                    "background": "#FFDE57",
                    "properties": {
                        "prefix": " \uE235 ",
                        "display_mode": "files",
                        "fetch_virtual_env": false,
                        "template": "{{ .Full }}"
                    }
                },
                {
                    "type": "azfunc",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ffffff",
                    "background": "#FEAC19",
                    "properties": {
                        "prefix": " \uf0e7",
                        "display_version": false,
                        "display_mode": "files"
                    }
                },
                {
                    "type": "aws",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ffffff",
                    "background_templates": [
                        "{{if contains \"default\" .Profile}}#FFA400{{end}}",
                        "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
                    ],
                    "properties": {
                        "prefix": " \uE7AD ",
                        "display_default": false
                    }
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#111111",
                    "background": "#BD3C47",
                    "properties": {
                        "root_icon": ""
                    }
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#ffffff",
                    "background": "#83769c",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "always_enabled": true,
                        "prefix": "<transparent>\uE0B0</> \ufbab",
                        "postfix": "\u2800"
                    }
                },
                {
                    "type": "exit",
                    "style": "diamond",
                    "foreground": "#ffffff",
                    "background": "#00897b",
                    "background_templates": ["{{ if gt .Code 0 }}#e91e63{{ end }}"],
                    "leading_diamond": "",
                    "trailing_diamond": "\uE0B4",
                    "properties": {
                        "always_enabled": true,
                        "template": "\uE23A",
                        "prefix": "<parentBackground>\uE0B0</> "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [{
                "type": "text",
                "style": "plain",
                "foreground": "#33658A",
                "properties": {
                    "prefix": "",
                    "postfix": "",
                    "text": "╰─\uE0B0"
                }
            }]
        }
    ],
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{ .Shell }} in {{ .Folder }}"
}